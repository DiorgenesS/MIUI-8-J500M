#	mount	point
		
/boot	emmc	/dev/block/bootdevice/by-name/boot
/recovery	emmc	/dev/block/bootdevice/by-name/recovery
/system	ext4	/dev/block/bootdevice/by-name/system
/cache	ext4	/dev/block/bootdevice/by-name/cache
/data	ext4	/dev/block/bootdevice/by-name/userdata
/preload	ext4	/dev/block/bootdevice/by-name/hidden
/persist	ext4	/dev/block/bootdevice/by-name/persist
/firmware	ext4	/dev/block/bootdevice/by-name/modem
/modem	emmc	/dev/block/bootdevice/by-name/modem
/efs	emmc	/dev/block/mmcblk0p13
/external_sd	vfat	/dev/block/mmcblk1p1
/usb-otg	vfat	/dev/block/sda1
